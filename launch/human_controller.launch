<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <group ns = "human">
    <!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam file="$(find imu_human_pkg)/config/human_control.yaml" command="load"/>

 </group>

     <!-- Load the controllers -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="joint_1_1_position_controller joint_1_2_position_controller joint_1_3_position_controller joint_state_controller"/>

</launch>

